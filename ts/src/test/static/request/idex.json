{
    "exchange": "idex",
    "skipKeys": [
        "nonce"
    ],
    "outputType": "json",
    "methods": {
        "fetchFundingRateHistory": [
            {
                "description": "Fetch funding rate history",
                "method": "fetchFundingRateHistory",
                "url": "https://api.idex.io/v4/fundingRates?market=ETH-USD",
                "input": [
                    "ETH/USD"
                ],
                "output": null
            }
        ],
        "fetchOrderBook": [
            {
                "description": "Fetch order book",
                "method": "fetchOrderBook",
                "url": "https://api.idex.io/v4/orderbook?market=ETH-USD&level=2",
                "input": [
                    "ETH/USD"
                ],
                "output": null
            }
        ],
        "fetchTrades": [
            {
                "description": "Fetch trades",
                "method": "fetchTrades",
                "url": "https://api.idex.io/v4/trades?market=ETH-USD",
                "input": [
                    "ETH/USD"
                ],
                "output": null
            }
        ],
        "fetchOHLCV": [
            {
                "description": "Fetch OHLCV",
                "method": "fetchOHLCV",
                "url": "https://api.idex.io/v4/candles?market=ETH-USD&interval=1m",
                "input": [
                    "ETH/USD",
                    "1m"
                ],
                "output": null
            }
        ],
        "fetchTickers": [
            {
                "description": "Fetch tickers",
                "method": "fetchTickers",
                "url": "https://api.idex.io/v4/tickers",
                "input": [],
                "output": null
            }
        ],
        "fetchTime": [
            {
                "description": "Fetch time",
                "method": "fetchTime",
                "url": "https://api.idex.io/v4/time",
                "input": [],
                "output": null
            }
        ]
    }
}
