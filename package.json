    "asciichart": "^1.5.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-es2017-object-entries": "0.0.4",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "nyc": "^11.0.3",
    "ololog": "^1.0.11"
  },
