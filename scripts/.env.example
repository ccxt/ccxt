# CoW Protocol CCXT Integration Test Configuration
# Copy this file to .env and fill in your values

# Wallet Configuration
WALLET_ADDRESS=0xYourWalletAddress
PRIVATE_KEY=0xYourPrivateKey

# Network Configuration
# Options: mainnet, xdai, arbitrum_one, base, sepolia
NETWORK=sepolia
ENV=barn

# Test Configuration
# Symbol to use for order tests (e.g., WETH/USDC)
TEST_SYMBOL=USDC/DAI
# Small amount for test orders (in base currency)
TEST_AMOUNT=0.001
# Test order price (for limit orders)
TEST_PRICE=2000
